{
	"logger": {
		"level": "debug",
		"core": [{
			"formatter": {
				"type": "string",
				"pattern": "{timestamp:l} {trace_id:{0:default}0>16}/{thread:d}/{process} {severity}: {message}, attrs: [{...}]",
				"sevmap": ["DEBUG", "NOTICE", "INFO", "WARNING", "ERROR"]
			},
			"sinks": [{
				"type": "asynchronous",
				"factor": 20,
				"overflow": "drop",
				"sink": {
					"type": "file",
					"path": "/dev/stderr",
					"flush": 1
				}
			}]
		}],
		"access": [{
			"formatter": {
				"type": "tskv",
				"create": {
					"tskv_format": "YT"
				}
			},
			"sinks": [{
				"type": "asynchronous",
				"factor": 20,
				"overflow": "drop",
				"sink": {
					"type": "file",
					"path": "/dev/stderr",
					"flush": 1
				}
			}]
		}]
	},
	"options": {
		"join": true,
		"flags": 20,
		"remote": [
			"autodiscovery:224.0.0.5:1025:2"
		],
		"address": [
			"localhost:1025:2-0"
		],
		"wait_timeout": 60,
		"check_timeout": 60,
		"io_thread_num": 16,
		"stall_count": 3,
		"nonblocking_io_thread_num": 16,
		"net_thread_num": 4,
		"daemon": false,
		"parallel": true,
		"auth_cookie": "qwerty",
		"bg_ionice_class": 3,
		"bg_ionice_prio": 0,
		"server_net_prio": 1,
		"client_net_prio": 6,
		"cache": {
			"size": 68719476736
		},
		"monitor": {
			"port":20000
		}
	},
	"backends": [
		{
			"backend_id": 1,
			"type": "blob",
			"group": 2,
			"history": "/opt/elliptics/history.2",
			"data": "/opt/elliptics/eblob.2/data",
			"sync": "-1",
			"blob_flags": "158",
			"blob_size": "10G",
			"records_in_blob": "1000000",
			"periodic_timeout": 15,
			"read_only": false,
			"datasort_dir": "/opt/elliptics/defrag/"
		}
	]
}
